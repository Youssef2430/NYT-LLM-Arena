{
  "name": "nyt-arena",
  "version": "1.0.0",
  "description": "LLM Benchmark for NYT Crosswords and Connections puzzles",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "bin": {
    "nyt-arena": "./src/cli/index.ts"
  },
  "scripts": {
    "cli": "bun run src/cli/index.ts",
    "run:suite": "bun run src/cli/index.ts run",
    "normalize": "bun run src/cli/index.ts normalize",
    "normalize:connections": "bun run src/data/normalize-connections.ts",
    "list": "bun run src/cli/index.ts list",
    "models": "bun run src/cli/index.ts models",
    "test": "bun test",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^25.0.3",
    "pino-pretty": "^13.1.3",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@types/react": "^19.2.7",
    "chalk": "^5.6.2",
    "cli-boxes": "^4.0.1",
    "commander": "^14.0.2",
    "figures": "^6.1.0",
    "ink": "^6.5.1",
    "ink-spinner": "^5.0.0",
    "ink-table": "^3.1.0",
    "nanoid": "^5.1.6",
    "p-limit": "^7.2.0",
    "pino": "^10.1.0",
    "react": "^19.2.3",
    "zod": "^4.2.1"
  }
}
