{
  "name": "connections-test",
  "description": "Test suite for Connections puzzle benchmark",

  "models": [
    "openai/gpt-oss-20b:free",
    "google/gemini-3-flash-preview",
    "openai/gpt-5.2-chat"
  ],

  "puzzles": {
    "type": "connections",
    "dateRange": {
      "start": "2023-06-12",
      "end": "2023-06-20"
    },
    "limit": 5,
    "shuffle": false
  },

  "repeats": 1,

  "maxSteps": 20,
  "runTimeoutMs": 180000,
  "stepTimeoutMs": 30000,

  "maxConcurrentRuns": 3,
  "maxConcurrentRequests": 5,

  "openRouter": {
    "includeUsage": true,
    "temperature": 0,
    "maxTokens": 512
  },

  "stepsCompression": "auto",
  "stepsCompressionThresholdBytes": 1000000,

  "maxInvalidActions": 5
}
